    "@tailwindcss/postcss": "4.1.13",
    "@wavesurfer/react": "1.0.11",
    "animejs": "3.2.2",
    "canvas": "3.2.0",
    "concurrently": "9.2.1",
    "electron-serve": "3.0.0",
    "electron-store": "10.1.0",
    "eslint": "9.36.0",
    "eslint-config-next": "15.5.4",
    "fabric": "5.5.1",
    "fabric-guideline-plugin": "0.0.11",
    "glob": "11.0.3",
    "heroicons": "2.2.0",
    "html-to-image": "1.11.11",
    "idb": "8.0.3",
    "mafs": "0.21.0",
    "mobx": "6.13.7",
    "mobx-react": "9.2.0",
    "mobx-react-lite": "4.1.0",
    "onnxruntime-web": "1.21.0",
    "pako": "2.1.0",
    "react-icons": "5.5.0",
    "react-modal": "3.16.3",
    "rimraf": "6.0.1",
    "wavesurfer.js": "7.10.2",
    "webm-duration-fix": "1.0.4"
  },
  "devDependencies": {
    "@electron/rebuild": "4.0.1",
