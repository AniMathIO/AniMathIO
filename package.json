    "@types/react-modal": "3.16.3",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/ui": "3.0.8",
    "autoprefixer": "10.4.20",
    "concurrently": "9.1.0",
    "electron": "35.0.1",
    "electron-builder": "26.0.11",
    "happy-dom": "17.4.3",
