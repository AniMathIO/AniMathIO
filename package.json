    "@types/animejs": "3.1.13",
    "@types/estree": "1.0.8",
    "@types/fabric": "5.3.10",
    "@types/json-schema": "7.0.15",
    "@types/katex": "0.16.7",
    "@types/node": "24.5.2",
    "@types/pako": "2.0.4",
    "@types/react": "19.1.13",
    "@types/react-color": "3.0.13",
    "@types/react-dom": "19.1.9",
    "@types/react-modal": "3.16.3",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.1",
    "electron": "38.1.2",
    "electron-builder": "26.0.20",
    "happy-dom": "18.0.1",
