{
  "private": true,
  "name": "manim-studio-next",
  "description": "ManimStudioNext is a video editor application for editing Manim-like video projects ",
  "version": "1.0.0",
  "author": "Kovacs Balint-Hunor",
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "nextron build",
    "build:prod": "nextron build --config electron-builder.prod.yml",
    "build:mac": "nextron build --mac --x64",
    "build:mac:universal": "nextron build --mac --universal",
    "build:win": "nextron build --win --ia32",
    "build:linux": "nextron build --linux",
    "build-native": "next build renderer && electron-builder",
    "postinstall": "electron-builder install-app-deps && npm rebuild canvas --update-binary"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@ffmpeg/util": "^0.12.1",
    "@heroicons/react": "^2.1.3",
    "@wavesurfer/react": "^1.0.5",
    "animejs": "^3.2.2",
    "concurrently": "^8.2.2",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.1.0",
    "eslint-config-next": "^13.5.6",
    "fabric": "^5.3.0",
    "heroicons": "^2.1.3",
    "mafs": "^0.18.4",
    "mobx": "^6.12.3",
    "mobx-react": "^9.1.1",
    "react-color": "^2.19.3"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.12",
    "@types/fabric": "^5.3.7",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/react-color": "^3.0.12",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^29.3.0",
    "electron-builder": "^24.9.1",
    "next": "^13.5.6",
    "nextron": "^8.13.0",
    "postcss": "^8.4.30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
