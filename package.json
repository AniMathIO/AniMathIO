    "@imgly/background-removal": "1.6.0",
    "@wavesurfer/react": "1.0.7",
    "animejs": "3.2.2",
    "canvas": "2.11.2",
    "concurrently": "9.1.0",
    "electron-serve": "1.3.0",
    "electron-store": "8.2.0",
    "eslint": "9.16.0",
    "eslint-config-next": "15.0.3",
    "fabric": "5.5.1",
    "fabric-guideline-plugin": "0.0.11",
    "glob": "9.3.5",
    "heroicons": "2.2.0",
    "html-to-image": "1.11.11",
    "idb": "8.0.0",
    "mafs": "0.21.0",
    "mobx": "6.13.5",
    "mobx-react": "9.1.1",
    "mobx-react-lite": "4.0.7",
    "onnxruntime-web": "1.21.0-dev.20250206-d981b153d3",
    "pako": "2.1.0",
    "react-icons": "5.3.0",
    "react-modal": "3.16.1",
    "rimraf": "6.0.1",
    "wavesurfer.js": "7.9.1",
    "webm-duration-fix": "1.0.4"
  },
  "devDependencies": {
    "@electron/rebuild": "3.7.1",
